% Custom Beamer Theme - Master File
% beamerthemecustom.sty

\mode<presentation>

% Load required packages
\RequirePackage{tikz}
\RequirePackage{xcolor}

% Load sub-themes
\usecolortheme{custom}
\usefonttheme{custom}
\useinnertheme{custom}
\useoutertheme{custom}

% No navigation symbols
\setbeamertemplate{navigation symbols}{}

\mode<all>